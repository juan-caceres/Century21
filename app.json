{
  "expo": {
    "name": "C21-Salas",
    "slug": "century21",
    "version": "1.0.0",
    "scheme": "century21",
    "orientation": "portrait",
    "icon": "./assets/century21-logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/century21-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.century21.miapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/century21-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.juancruzcaceres.century21"
    },
    "notification": {
      "icon": "./assets/century21-logoTransparente2.png",
      "color": "#ffffff",
      "androidMode": "default",
      "androidCollapsedTitle": "Nuevas notificaciones"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d34372cc-5926-4b50-8f4a-1b0a29d49f6c"
      }
    },
    "owner": "juancruzcaceres",
    "plugins": [
      "expo-font"
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/d34372cc-5926-4b50-8f4a-1b0a29d49f6c"
    }
  }
}
